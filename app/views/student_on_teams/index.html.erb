<p id="notice"><%= notice %></p>

<h1>Student On Teams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_on_teams.each do |student_on_team| %>
      <tr>
        <td><%= link_to 'Show', student_on_team %></td>
        <td><%= link_to 'Edit', edit_student_on_team_path(student_on_team) %></td>
        <td><%= link_to 'Destroy', student_on_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

